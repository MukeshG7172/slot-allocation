generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Slot {
  id          String   @id @default(cuid())
  title       String
  startTime   DateTime
  endTime     DateTime
  testLink    String
  description String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}